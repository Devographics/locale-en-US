root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

# YAML locale files may contain intentional trailing spaces
# inside multiline blocks (e.g. two trailing spaces = Markdown <br>).
# Do NOT trim them automatically.
[*.yml]
trim_trailing_whitespace = false